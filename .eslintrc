{
  "extends": ["airbnb"],
  "quotes": [2, "single", "avoid-escape"],
  "plugins": [
    "meteor"
  ],
  "env": {
    "meteor": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "react/prefer-es6-class": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  }
}
